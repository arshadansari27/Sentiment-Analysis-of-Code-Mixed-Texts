{\trowd\clbrdrl\brdrs\clbrdrr\brdrs\cellx9104
{\pard\intbl\ql { if Leh is None:}\cell}
\row}
{\trowd\clbrdrl\brdrs\clbrdrr\brdrs\cellx9104
{\pard\intbl\ql {Leh = Qeh(Lh{i}) for i in Lh}\cell}
\row}
{\trowd\clbrdrl\brdrs\clbrdrr\brdrs\cellx9104
{\pard\intbl\ql {if Lem is None:}\cell}
\row}
{\trowd\clbrdrl\brdrs\clbrdrr\brdrs\cellx9104
{\pard\intbl\ql { Lem = Qem(Lm{i}) for i in Lm}\cell}
\row}
{\trowd\clbrdrl\brdrs\clbrdrr\brdrs\cellx9104
{\pard\intbl\ql {if not model:}\cell}
\row}
{\trowd\clbrdrl\brdrs\clbrdrr\brdrs\cellx9104
{\pard\intbl\ql {Model = CRF(FNGram(Leh), FNGram(Lem), FNGram(Le))}\cell}
\row}
{\trowd\clbrdrl\brdrs\clbrdrr\brdrs\cellx9104
{\pard\intbl\ql {if not w in D:}\cell}
\row}
{\trowd\clbrdrl\brdrs\clbrdrr\brdrs\cellx9104
{\pard\intbl\ql {w = stem word(w)}\cell}
\row}
{\trowd\clbrdrl\brdrs\clbrdrr\brdrs\cellx9104
{\pard\intbl\ql {if w not in D:}\cell}
\row}
{\trowd\clbrdrl\brdrs\clbrdrr\brdrs\cellx9104
{\pard\intbl\ql {w = find most similar word(D, w)}\cell}
\row}
{\trowd\clbrdrl\brdrs\clbrdrr\brdrs\cellx9104
{\pard\intbl\ql {l = arg max(Model, sentence, w)}\cell}
\row}
{\trowd\clbrdrl\brdrs\clbrdrb\brdrs\clbrdrr\brdrs\cellx9104
{\pard\intbl\ql {return l}\cell}
\row}
